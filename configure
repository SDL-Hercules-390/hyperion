   darwin*)
       if test -n "$STRIP" ; then
         striplib="$STRIP -x"
         { $as_echo "$as_me:$LINENO: result: yes" >&5
$as_echo "yes" >&6; }
       else
  { $as_echo "$as_me:$LINENO: result: no" >&5
$as_echo "no" >&6; }
