        if test -d "${hc_cv_extpkg_dir}"; then
          { $as_echo "$as_me:$LINENO: Using External Packages override directory ${hc_cv_extpkg_dir} " >&5
$as_echo "$as_me: Using External Packages override directory ${hc_cv_extpkg_dir} " >&6;}
        else
          { $as_echo "$as_me:$LINENO: result: ERROR: --enable-extpkgs directory not found: ${hc_cv_extpkg_dir} " >&5
$as_echo "ERROR: --enable-extpkgs directory not found: ${hc_cv_extpkg_dir} " >&6; }
          hc_error=yes
          hc_cv_extpkg_dir=""
        fi

