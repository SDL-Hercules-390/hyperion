            hc_cv_have_hqa_h=no
          fi
        else
          { $as_echo "$as_me:$LINENO: result: ERROR: --enable-hqa directory not found: $hc_cv_hqa_opt " >&5
$as_echo "ERROR: --enable-hqa directory not found: $hc_cv_hqa_opt " >&6; }
          hc_error=yes
          hc_cv_have_hqa_h=no
        fi

        if test "$hc_cv_have_hqa_h" = "yes"; then
          if test "$hc_cv_hqa_inc" != "."; then
            { $as_echo "$as_me:$LINENO: result: WARNING: --enable-hqa value overrides existing HAQ_DIR value " >&5
$as_echo "WARNING: --enable-hqa value overrides existing HAQ_DIR value " >&6; }
